// CHECK:         object: {
// CHECK:             exprType: "Parameter",
// CHECK:             position: 0
// CHECK:         },
// CHECK:         property: "age"
// CHECK:     },
// CHECK:     op: "Gt",
// CHECK:     right: {
// CHECK:         exprType: "Literal",
// CHECK:         value: 40
// CHECK:     }
// CHECK: }, (person)=>{
// CHECK:     return fetch("foobar");
// CHECK: });
